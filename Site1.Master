<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site1.master.cs" Inherits="WebApplication.Site1" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <link href="masterPage.css" rel="stylesheet" />
    <title></title>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="form1" runat="server">
        <div id="header">
            <table id="headerTb">
                <tr>
                    <td>
                        <h2>The Secret Garden Artwork Gallery</h2>
                    </td>
                    <td id="headerBtn">
                        
                        <asp:Button ID="wishlishBtn" runat="server" Text="❤" OnClick="wishlishBtn_Click" />
                        &nbsp;
                        <asp:Button ID="cartBtn" runat="server" Text="My Cart" OnClick="cartBtn_Click" />
                        &nbsp;
                        <asp:Button ID="artBtn" runat="server" Text="My Art" OnClick="artBtn_Click" />
                        &nbsp;
                        <asp:Button ID="accBtn" runat="server" Text="Profile" OnClick="accBtn_Click" />
                        &nbsp;&nbsp;
                      <b> Hi, <asp:LoginName ID="LoginName1" runat="server" /></b>
                        &nbsp;&nbsp;
                        <asp:LoginStatus ID="LoginStatus1" runat="server" LogoutAction="RedirectToLoginPage" />
                    </td>
                </tr>
            </table>
        </div>

        <br />

        <div style="margin:10px 0px 50px 20px">
            <asp:SiteMapPath ID="SiteMapPath1" runat="server"></asp:SiteMapPath>
            
            <asp:SiteMapDataSource ID="SiteMapDataSource1" runat="server" />
        </div>
        <div id="content">
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
            </asp:ContentPlaceHolder>
            
        </div>
        <div id="footer">
            <h3>Footer</h3>
        </div>
    </form>
</body>
</html>
